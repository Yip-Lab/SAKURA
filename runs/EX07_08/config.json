{
  "remarks": "This experiment is forked from EX07_03, but shuffled phenotype labels, and classification of shuffled labels is intensified (1, 1.1, 1, 10)",
  "log_path": "./runs/EX07_08/logs/",
  "reproducible": "True",
  "rnd_seed": 114514,
  "dataset": {
    "type": "rna_count_sparse",
    "gene_expr_MM_path": "./dataset/10X_Purified_PBMC/cleaned_pooled_log_count_dgC_unfiltered.mtx",
    "gene_name_csv_path": "./dataset/10X_Purified_PBMC/cleaned_pooled_log_count_dgC_unfiltered_gene_names.csv",
    "cell_name_csv_path": "./dataset/10X_Purified_PBMC/cleaned_pooled_log_count_dgC_unfiltered_cell_names.csv",
    "pheno_csv_path": "./dataset/10X_Purified_PBMC/shuffled_pooled_pheno.csv",
    "pheno_meta_path": "./runs/EX07_08/pheno_meta.json",
    "expr_mat_pre_slice": "True",
    "selected_pheno": [
      "data_key"
    ]
  },
  "device": "cpu",
  "overall_train_test_split": {
    "type": "auto",
    "train_dec": 8,
    "seed": 1919
  },
  "main_latent": {
    "encoder_neurons": 50,
    "decoder_neurons": 50,
    "latent_dim": 2,
    "loss": {
      "L2": {
        "type": "MSE",
        "init_weight": 1.0,
        "progressive_const": 1.0,
        "progressive_start_epoch": 100
      },
      "L1": {
        "type": "L1",
        "init_weight": 1.0,
        "progressive_const": 1.0,
        "progressive_start_epoch": 100
      }
    },
    "regularization": {
      "uniform_shape_unsupervised": {
        "type": "SW2_uniform",
        "init_weight": 1,
        "progressive_const": 1.1,
        "progressive_start_epoch": 1,
        "max_weight": 10.0,
        "SW2_num_projections": 50,
        "uniform_low": -10,
        "uniform_high": 10
      }
    }
  },
  "optimizer": {
    "type": "RMSProp",
    "RMSProp_lr": 0.001,
    "RMSProp_alpha": 0.9
  },
  "dump_configs": "True",
  "dump_splits": "True",
  "story": [
    {
      "action": "train",
      "epochs": 100,
      "use_split": "overall_train",
      "train_main_latent": "True",
      "train_pheno": "True",
      "train_signature": "False",
      "remark": "Train main latent and phenotype dimensions",
      "batch_size": 100,
      "make_logs": "True",
      "log_prefix": "overall_train",
      "test_every_epoch": "True",
      "tests": [
        {
          "on_split": "overall_test",
          "make_logs": "True",
          "log_prefix": "overall_test",
          "dump_latent": "True",
          "latent_prefix": "overall_test_all_latent"
        },
        {
          "on_split": "overall_train",
          "make_logs": "False",
          "log_prefix": "overall_test",
          "dump_latent": "True",
          "latent_prefix": "overall_train_all_latent"
        }
      ]
    }
  ]
}